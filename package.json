    "@typescript-eslint/parser": "^4.4.1",
    "actions-on-google": "3.0.0",
    "eslint": "^7.11.0",
    "hap-node-client": "0.1.5",
    "hap-nodejs": "^0.8.2",
    "homebridge": "^1.3.3",
    "nodemon": "^2.0.5",
    "request-promise": "0.0.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "bundleDependencies": [
    "@oznu/ws-connect",
    "fs-extra",
    "hap-node-client",
