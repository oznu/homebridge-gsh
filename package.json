    "build:ui": "npm run build --prefix homebridge-ui/public",
    "lint": "eslint --ext .ts --max-warnings=0 .",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=10.17.0",
    "homebridge": ">=1.0.0"
  },
  "funding": [
    {
      "type": "paypal",
      "url": "https://paypal.me/oznu"
    }
  ],
  "keywords": [
    "homebridge-plugin",
    "google smart home",
    "google-smart-home",
    "google home",
    "google-home",
    "google assistant",
    "google-assistant",
    "google",
    "android",
    "homebridge",
    "gsh"
  ],
  "bundledDependencies": [
    "@oznu/ws-connect",
    "fs-extra",
    "hap-node-client",
    "rxjs"
  ],
  "dependencies": {
    "@google-cloud/local-auth": "3.0.1",
    "@oznu/ws-connect": "^2.2.0",
    "axios": "1.6.8",
